{
  "name": "kento-api",
  "version": "0.0.1",
  "description": "",
  "author": "kento",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pretty": "pnpm exec prettier --ignore-path .prettierignore --cache --no-error-on-unmatched-pattern --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "db:cli": "mikro-orm",
    "migration:initial": "mikro-orm migration:create --initial",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:list": "mikro-orm migration:list",
    "migration:check": "mikro-orm migration:check",
    "migration:pending": "mikro-orm migration:pending",
    "migration:fresh": "mikro-orm migration:fresh",
    "seed:initial": "mikro-orm seeder:run",
    "commit": "git-cz",
    "husky:i": "husky init",
    "up:i": "pnpm up --interactive",
    "up:f": "pnpm up --latest",
    "prepare": "husky"
  },
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@mikro-orm/core": "^6.2.1",
    "@mikro-orm/nestjs": "^5.2.3",
    "@mikro-orm/postgresql": "^6.2.1",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.7",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/format": "^19.0.3",
    "@mikro-orm/cli": "^6.2.1",
    "@mikro-orm/migrations": "^6.2.1",
    "@mikro-orm/seeder": "^6.2.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.13",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^2.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "unplugin-swc": "^1.4.5",
    "vitest": "^1.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "formatter": "@commitlint/format",
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.ts": [
      "pnpm lint"
    ]
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "@apollo/server-plugin-landing-page-graphql-playground": "4.0.0",
      "subscriptions-transport-ws": "0.11.0"
    }
  },
  "mikro-orm": {
    "useTsNode": true
  }
}
