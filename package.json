{
  "name": "kento-api",
  "version": "0.0.1",
  "description": "",
  "author": "kento",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pretty": "pnpm exec prettier --ignore-path .prettierignore --cache --no-error-on-unmatched-pattern --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "db:cli": "mikro-orm",
    "migration:initial": "mikro-orm migration:create --initial",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:list": "mikro-orm migration:list",
    "migration:check": "mikro-orm migration:check",
    "migration:pending": "mikro-orm migration:pending",
    "migration:fresh": "mikro-orm migration:fresh",
    "seed:initial": "mikro-orm seeder:run",
    "commit": "git-cz",
    "husky:i": "husky install",
    "update:i": "pnpm up --interactive",
    "update:f": "pnpm up --latest"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@mikro-orm/core": "^5.8.2",
    "@mikro-orm/nestjs": "^5.2.2",
    "@mikro-orm/postgresql": "^5.8.2",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@mikro-orm/sqlite": "^5.8.2",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.6",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "graphql": "^16.8.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/format": "^17.4.4",
    "@mikro-orm/cli": "^5.8.2",
    "@mikro-orm/migrations": "^5.8.2",
    "@mikro-orm/seeder": "^5.8.2",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.88",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.18",
    "@types/node": "^20.6.4",
    "@types/passport-jwt": "^3.0.10",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-perfectionist": "^2.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "unplugin-swc": "^1.4.2",
    "vitest": "^0.34.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "formatter": "@commitlint/format",
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.ts": [
      "pnpm lint"
    ]
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "@apollo/server-plugin-landing-page-graphql-playground": "4.0.0",
      "subscriptions-transport-ws": "0.11.0"
    }
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/database/settings.ts",
      "./dist/database/settings.js"
    ]
  }
}
